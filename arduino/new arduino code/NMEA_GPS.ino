##include "SoftwareSerial.h"#include "TinyGPS.h"#define RXPIN 3#define TXPIN 2SoftwareSerial serial(RXPIN, TXPIN);TinyGPS gps;long lat, lon;unsigned long age;void setup() {    Serial.begin(9600);    serial.being(9600); // we can run both at the same time}void loop(){    while (serial.available())    {        int c = serial.read();        if (gps.encode(c))        {            gps.get_position(&lat, &lon, &age);            Serial.println(" ---------- ");            Serial.println(" latitude ");            Serial.println(lat);            Serial.println(" longitude ");            Serial.println(lon);            Serial.println(" age ");            Serial.println(age);        }    }}